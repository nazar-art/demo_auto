<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
	       http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.1.xsd"
       profile="defaultTest">

    <context:annotation-config />
    <bean class="org.springframework.beans.factory.annotation.AutowiredAnnotationBeanPostProcessor"/>

    <!--DAO LAYER-->
    <bean id="daoFactory" class="com.epam.model.dao.modules.DAOFactory"/>

    <bean id="generatorDAO" class="com.epam.model.dao.generator.GeneratorDAO"/>

    <bean id="catalogueManagementDAO"
          class="com.epam.model.dao.modules.catalogue.CatalogueManagementDAO" />


    <!--todo remove later - this is test stub-->
    <bean id="adminDao" class="com.epam.model.dao.modules.admin.AdminDAO"/>
</beans>
